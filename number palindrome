class HelloWorld {
    static void Main() {
        int n = 12322;
        int temp=n;
        int rem =0;
        int res =0;
        do
        {
            rem=n%10;
            n=n/10;
            res=res * 10 + rem;
        }while(n!=0);
        if(temp==res)
        {
            Console.WriteLine("Palindrome Number");
        }
        else
        {
            Console.WriteLine("Not Palindrome Number");
        }   
    }
}
