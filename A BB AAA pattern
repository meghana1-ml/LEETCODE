class HelloWorld {
    static void Main() {
        int N=5;
        for (int i=0; i<N; i++)
        {
            for (int j=0; j<=i; j++)
            {
                if(i%2==0)
               Console.Write("A");
            else{
                Console.Write("B");
            }
            }
            Console.WriteLine();
        }       
    }
}
